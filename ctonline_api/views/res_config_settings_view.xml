<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
        <record id="ctonline_api_res_config_settings_view_form" model="ir.ui.view">
             <field name="name">res.config.settings</field>
             <field name="model">res.config.settings</field>
             <field name="priority" eval="100"/>
             <field name="inherit_id" ref="base.res_config_settings_view_form"/>
             <field name="arch" type="xml">
                 <xpath expr="//form" position="inside">
                     
                     <app data-string="ctonline_api" string="CTonline" name="ctonline_api">
                        <block title="Acces api" id="acces_api">
                            <setting id="x_api_key">
                                <field name="x_api_key"/>
                            </setting>
                            
                        </block>
                        
                    </app>
                 </xpath>
             </field>
         </record>


        

    
</odoo>