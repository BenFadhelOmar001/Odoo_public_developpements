<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="custom_stt_view_move_form" model="ir.ui.view">
        <field name="name">account.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">


            <xpath expr="//notebook/page[@name='other_info']/group[1]/group[1]/field[@name='partner_bank_id']" position="after">
                
                <field name="total_en_dinar" string="Total en Dinar"/>

                <field name="montant_tva_exonere" string="Montant TVA19% exonéré"/>

                <field name="mt_regle_dnt" string="mt_regle_dnt" invisible="1"/>
                <field name="solde_due_dnt" string="solde_due_dnt" invisible="1"/>
                

                
            </xpath>
        </field>
    </record>

    
</odoo>
